<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.dashboard.StatisticsFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <com.github.mikephil.charting.charts.LineChart
            android:id="@+id/chart"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintBaseline_toBottomOf="@id/toolbar">

        </com.github.mikephil.charting.charts.LineChart>

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rvGarages"
            android:layout_width="match_parent"
            android:layout_height="300dp" />

        <androidx.fragment.app.FragmentContainerView
            android:id="@+id/parked_status_container"
            android:name="com.cs407.madparking.ui.dashboard.statis.OnParkedFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>